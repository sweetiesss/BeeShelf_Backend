{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7001",
    "Hosts": {
      "Service": "https://beeshelfapi.azurewebsites.net"
    },
    "LoadBalancerOptions": {
      "Type": "RoundRobin"
    },
    "HealthCheckOptions": {
      "Timeout": 5000,
      "Interval": 10000,
      "Retries": 3,
      "Path": "/health"
    },
    "RateLimitOptions": {
      "ClientWhitelist": [],
      "EnableRateLimiting": true,
      "Period": "5s",
      "PeriodTimespan": 3,
      "Limit": 3
    }
  },
