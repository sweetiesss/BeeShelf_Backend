{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7001"
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/gateway/{everything}",
      "UpstreamHttpMethod": [ "Get" ],
      "UpstreamHeaderTransform": {
        "X-Api-Key": "b3e4f7b76e344c90bc5e1f1d8d6c2f98"
      },
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7092
        }
      ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      },
      "HealthCheckOptions": {
        "Timeout": 5000,
        "Interval": 10000,
        "Retries": 3,
        "Path": "/health"
      },
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "5s",
        "PeriodTimespan": 3,
        "Limit": 3
      }
    }
  ]
}